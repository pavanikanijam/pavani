if(a>b):
  elif(a>c):
    print("a is greater:")
  else:
  print("c is greater")
elif(b>a):
  elif(b>c):
    print("b is greater")
  else:
  print("c is greater")
else:
print("a is greater")
else:
print(b is greater")
